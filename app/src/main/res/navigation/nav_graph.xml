<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/exampleFragment">

    <!--<include app:graph="@navigation/home_nav_graph" />-->

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.san.archapp.ui.home.HomeFragment"
        android:label="fragment_users"
        tools:layout="@layout/fragment_users">


        <action
            android:id="@+id/action_homeFragment_to_commentFragment"
            app:destination="@id/commentFragment" />
    </fragment>

    <fragment
        android:id="@+id/commentFragment"
        android:name="com.san.archapp.ui.home.comment.CommentFragment"
        android:label="CommentFragment"
        tools:layout="@layout/fragment_comment">
        <argument
            android:name="id"
            app:argType="integer" />
    </fragment>
    <fragment
        android:id="@+id/exampleFragment"
        android:name="com.san.archapp.ui.examples.ExamplesFragment"
        android:label="fragment_examples"
        tools:layout="@layout/fragment_examples" >
        <action
            android:id="@+id/action_exampleFragment_to_serviceFragment"
            app:destination="@id/serviceFragment" />
        <action
            android:id="@+id/action_exampleFragment_to_webViewFragment"
            app:destination="@id/webViewFragment" />
    </fragment>
    <fragment
        android:id="@+id/settingsFragment"
        android:name="com.san.archapp.ui.settings.SettingsFragment"
        android:label="fragment_settings"
        tools:layout="@layout/fragment_settings" />
    <fragment
        android:id="@+id/serviceFragment"
        android:name="com.san.archapp.ui.examples.service.ServiceFragment"
        android:label="ServiceFragment" />
    <fragment
        android:id="@+id/webViewFragment"
        android:name="com.san.archapp.ui.examples.webview.WebViewFragment"
        android:label="fragment_web_view"
        tools:layout="@layout/fragment_web_view" />

</navigation>